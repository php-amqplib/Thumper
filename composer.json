{
    "name": "php-amqplib/thumper",
    "replace": {
        "videlalvaro/thumper": "self.version"
    },
    "license": "MIT",
    "type": "library",
    "description": "AMQP Tools",
    "keywords": [
        "library",
        "AMQP",
        "RabbitMQ",
        "PHPAMQPLib",
        "Thumper"
    ],
    "homepage": "https://github.com/php-amqplib/Thumper",
    "autoload": {
        "psr-0": {
            "Thumper": "lib"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Thumper\\Test\\": "tests"
        }
    },
    "require": {
        "php": "^5.3.3|^7.0|^8.0",
        "php-amqplib/php-amqplib": "^2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "scrutinizer/ocular": "^1.2",
        "squizlabs/php_codesniffer": "^2.6"
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.5.0-dev"
        }
    }
}
